generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Dataa {
  id   Int    @id @default(autoincrement())
  text String @db.VarChar(255)
}

model Errors {
  id   Int    @id @default(autoincrement())
  err  Int
  text String @db.VarChar(255)
}

model Log {
  id  Int    @id @default(autoincrement())
  log String @db.VarChar(255)
}
